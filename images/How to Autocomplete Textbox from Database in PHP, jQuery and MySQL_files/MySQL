026gt;Delete\u0026lt;\/th\u0026gt;\u003Cbr \/\u003E                    \u0026lt;\/tr\u0026gt;\u003Cbr \/\u003E                \u0026lt;\/thead\u0026gt;\u003Cbr \/\u003E                \u0026lt;tbody\u0026gt;\u003Cbr \/\u003E                \u0026lt;?php\u003Cbr \/\u003E                $cnt = 1;\u003Cbr \/\u003E                while($row = mysqli_fetch_array($result)) { ?\u0026gt;\u003Cbr \/\u003E                \u0026lt;tr\u0026gt;\u003Cbr \/\u003E                    \u0026lt;td\u0026gt;\u0026lt;?php echo $cnt++; ?\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E                    \u0026lt;td\u0026gt;\u0026lt;?php echo $row['lname']; ?\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E                    \u0026lt;td\u0026gt;\u0026lt;?php echo $row['lcode']; ?\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E                    \u0026lt;td\u0026gt;\u0026lt;a href=\"update_language.php?langid=\u0026lt;?php echo $row['id']; ?\u0026gt;\"\u0026gt;\u0026lt;img src=\"images\/edit-icon.png\" alt=\"Edit\" \/\u0026gt;\u0026lt;\/a\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E                    \u0026lt;td\u0026gt;\u0026lt;a href=\"javascript: delete_lang(\u0026lt;?php echo $row['id']; ?\u0026gt;)\"\u0026gt;\u0026lt;img src=\"images\/delete-icon.png\" alt=\"Delete\" \/\u0026gt;\u0026lt;\/a\u0026gt;\u0026lt;\/td\u0026gt;\u003Cbr \/\u003E                \u0026lt;\/tr\u0026gt;\u003Cbr \/\u003E                \u0026lt;?php } ?\u0026gt;\u003Cbr \/\u003E                \u0026lt;\/tbody\u0026gt;\u003Cbr \/\u003E            \u0026lt;\/table\u0026gt;\u003Cbr \/\u003E            \u0026lt;div class=\"panel-footer\"\u0026gt;\u0026lt;?php echo mysqli_num_rows($result) . \" records found\"; ?\u0026gt;\u0026lt;\/div\u0026gt;\u003Cbr \/\u003E            \u0026lt;\/div\u0026gt;\u003Cbr \/\u003E        \u0026lt;\/div\u0026gt;\u003Cbr \/\u003E    \u0026lt;\/div\u0026gt;\u003Cbr \/\u003E\u0026lt;\/div\u0026gt;\u003Cbr \/\u003E\u0026lt;script src=\"js\/jquery-1.10.2.js\"\u0026gt;\u0026lt;\/script\u00